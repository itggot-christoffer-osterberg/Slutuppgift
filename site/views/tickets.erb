<div class="page-content">
  <h2 class="title">Mina Ã¤renden</h2>
  <% for ticket in @tickets %>
    <div class="ticket">
      <div class="question">
        <p class="ticket_status"><%= ticket.status.name %></p><br>
        <a class="ticket_link" href="/student/my_tickets/<%= ticket.id %>"><h3 class="ticket_header"><%= ticket.title %></h3></a>
        <p class="ticket_description"><%= ticket.description %></p>
      </div>
      <% for comment in ticket.comments %>
      <div class="comment">
        <p class="comment_description"><%= comment.description %></p>
      </div>
      <% end %>
    </div>
  <% end %>
</div>

<style>
      #my_tickets {
            background-color: #e6e6e6;
      }
</style>